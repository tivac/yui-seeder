{
  "name": "yui-seeder",
  "version": "0.0.0",
  "description": "Generate YUI module \"seed\" files, a collection of requirements",
  "main": "lib/seeder.js",
  "bin": {
    "seeder": "bin/cli.js"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run-script jscs && npm run-script jshint && npm run-script istanbul",
