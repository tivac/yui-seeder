  "bugs": {
    "url": "https://github.com/tivac/yui-seeder/issues"
  },
  "devDependencies": {
    "mocha": "~1.17.1",
    "istanbul": "~0.2.4",
    "jshint": "~2.4.4",
    "jscs": "~1.2.4",
    "nixt": "~0.3.0"
  },
  "dependencies": {
    "yui": "~3.14.1",
    "nomnom": "~1.6.2",
    "globule": "~0.2.0",
    "kat": "~0.3.0",
    "concat-stream": "~1.4.1",
